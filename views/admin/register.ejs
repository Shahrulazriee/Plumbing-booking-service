<%- include ("../partials/_headerBiasa")-%>
<h1>admin register page</h1>
<p>kene buat legals action and terms</p>
<div class="container1">
	<h1>Admin register page</h1>
	<form method="post" action="/api/admin/register">
		<label>Email address</label>
		<input
			name="email"
			type="text"
			placeholder="name@example.com"
			required
		/>
		<br />
		<label>Name</label>
		<input name="name" type="text" placeholder="****" required />
		<br />
		<label>Phone number</label>
		<input name="phone" type="text" placeholder="0123456789" required />
		<br />
		<label>Password</label>
		<input name="password" type="password" placeholder="*****" required />
		<br />
		<label>Re-enter password</label>
		<input name="password2" type="password" placeholder="*****" required />
		<br />
		<br />
		<button type="submit">Create Account</button>
		<br />
	</form>
</div>

<%- include ("../partials/_footer")-%>
